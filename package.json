{
  "name": "redux-popsicle",
  "version": "0.0.1",
  "description": "Manage network requests in redux actions with simple, testable object config",
  "main": "build/node.js",
  "scripts": {
    "build-node": "./node_modules/.bin/parcel build index.js --target node --out-file node.js",
    "build-browser": "./node_modules/.bin/parcel build index.js --target browser --out-file browser.js",
    "prepublish": "npm run build-node && npm run build-browser",
    "test": "./node_modules/.bin/jest --watch --env=jsdom"
  },
  "keywords": [
    "redux",
    "popsicle",
    "http",
    "REST",
    "json",
    "state",
    "management"
  ],
  "author": "@CAYdenberg",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^5.4.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^23.5.0",
    "parcel-bundler": "^1.9.7"
  },
  "dependencies": {
    "popsicle": "^10.0.1"
  }
}
