{
  "name": "redux-popsicle",
  "version": "0.1.1",
  "description": "Manage HTTP requests in redux actions with simple, testable object configs",
  "main": "build/node.js",
  "browser": "build/browser.js",
  "scripts": {
    "build-node": "./node_modules/.bin/parcel build src/index.js --target node --out-file node.js",
    "build-browser": "./node_modules/.bin/parcel build src/index.js --target browser --out-file browser.js",
    "prepare": "npm run build-node && npm run build-browser",
    "test": "./node_modules/.bin/jest --watch --env=jsdom"
  },
  "keywords": [
    "redux",
    "popsicle",
    "http",
    "REST",
    "json",
    "state",
    "management"
  ],
  "author": "@CAYdenberg",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.4.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^23.5.0",
    "parcel-bundler": "^1.9.7"
  },
  "dependencies": {
    "popsicle": "^10.0.1"
  },
  "repository": {
    "type" : "git",
    "url" : "https://github.com/CAYdenberg/redux-popsicle.git"
  },
  "bugs": {
    "url": "https://github.com/CAYdenberg/redux-popsicle/issues",
  },
  "jest": {
    "clearMocks": true
  }
}
